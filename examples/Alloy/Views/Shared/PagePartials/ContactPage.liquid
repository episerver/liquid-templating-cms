<div class="contact">
    <img class="mb-3" src="{{ Model.PageImage | url }}" />
    <h2>{{ Model.PageName }}</h2>
    <p>{{ Model.TeaserText }}</p>
    <p>
        <a class="d-block" href="mailto:{{ Model.Email }}">{{ Model.Email }}</a>
        <a class="d-block" href="tel:{{ Model.Phone}}">{{ Model.Phone }}</a>
    </p>
</div>
